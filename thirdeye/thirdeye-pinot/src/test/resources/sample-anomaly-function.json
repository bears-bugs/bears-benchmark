{
  "collection": "myCollection",
  "functionName": "impression percent threshold",
  "metric": "impressions",
  "metricFunction": "SUM",
  "type": "WEEK_OVER_WEEK_RULE",
  "isActive": true,
  "cron": "* 0/5 * * * ?",
  "properties": "changeThreshold=-0.10;averageVolumeThreshold=10000;baseline=w/w",
  "windowSize": 672,
  "windowUnit": "HOURS",
  "windowDelay": 3,
  "bucketSize": 1,
  "bucketUnit": "HOURS",
  "exploreDimensions": "source,countryCode",
  "filters": "dim1=value1;dim2=value2"
}
