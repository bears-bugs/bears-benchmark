<?xml version="1.0" encoding="UTF-8" ?>
<beans:beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xmlns:services="http://www.alibaba.com/schema/services"
             xmlns:beans="http://www.springframework.org/schema/beans"
             xmlns="http://www.alibaba.com/schema/services/uris"
             xsi:schemaLocation="http://www.alibaba.com/schema/services http://localhost:8080/schema/services.xsd
        http://www.alibaba.com/schema/services/uris http://localhost:8080/schema/services-uris.xsd

        http://www.springframework.org/schema/beans http://localhost:8080/schema/www.springframework.org/schema/beans/spring-beans.xsd">

    <services:uris>
        <uri id="server" exposed="true"></uri>

        <turbine-uri id="rootLink" exposed="true" extends="server">
            <componentPath>/</componentPath>
        </turbine-uri>

        <!--
        <servlet-content-uri id="rootContextPath" exposed="true" extends="rootLink">
        </servlet-content-uri>
        -->

        <turbine-uri id="servicemanageLink" exposed="true" extends="server">
            <componentPath>/servicemanage</componentPath>
        </turbine-uri>

        <turbine-uri id="serviceGovernance" exposed="true" extends="servicemanageLink">
            <target>serviceGovernance.vm</target>
        </turbine-uri>

        <turbine-uri id="registerProvider" exposed="true" extends="servicemanageLink">
            <target>registerProvider.vm</target>
        </turbine-uri>

    </services:uris>

</beans:beans>
