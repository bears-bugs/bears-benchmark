<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
  "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
  "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
  <class name="org.hisp.dhis.dataelement.CategoryOptionGroupSetDimension" table="categoryoptiongroupsetdimension">

    <cache usage="read-write" />

    <id name="id" column="categoryoptiongroupsetdimensionid">
      <generator class="native" />
    </id>
    
    <many-to-one name="dimension" class="org.hisp.dhis.dataelement.CategoryOptionGroupSet" column="categoryoptiongroupsetid"
      foreign-key="fk_dimension_categoryoptiongroupsetid" />
    
    <list name="items" table="categoryoptiongroupsetdimension_items">
      <cache usage="read-write" />
      <key column="categoryoptiongroupsetdimensionid" foreign-key="fk_dimension_items_categoryoptiongroupsetdimensionid" />
      <list-index column="sort_order" />
      <many-to-many class="org.hisp.dhis.dataelement.CategoryOptionGroup" column="categoryoptiongroupid"
        foreign-key="fk_dimension_items_categoryoptiongroupid" />
    </list>
  </class>
</hibernate-mapping>