<?xml version="1.0" encoding="utf-8" ?>
<beanmap xmlns="http://jsapar.tigris.org/BeanMapSchema/2.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://jsapar.tigris.org/BeanMapSchema/2.0 ../src/main/resources/xml/schema/BeanMapSchema.xsd ">
    <!--Bean line mappings without bean class name will be igored while parsing lines.-->
    <bean linetype="Header"/>
    <bean name="org.jsapar.TstPerson" linetype="Person">
        <property name="firstName" cellname="First name"/>
        <property name="lastName" cellname="Last name"/>
    </bean>
    <bean linetype="Footer"/>
</beanmap>