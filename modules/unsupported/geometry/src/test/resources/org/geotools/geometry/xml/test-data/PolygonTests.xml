<run>
<precisionModel type="FLOATING"/>

<case>
<desc>Polygon Tests</desc>
  <a>
    POLYGON ((160 500, 180 80, 780 80, 780 500, 620 500, 620 200, 320 200, 320 500, 320 500, 160 500))
  </a>
  <b>
    POLYGON ((60 440, 840 440, 840 280, 60 280, 60 280, 60 440))
  </b>
  <test>
    <op name="getboundary" arg1="A">
      LINEARRING (160 500, 180 80, 780 80, 780 500, 620 500, 620 200, 320 200, 320 500, 320 500, 160 500)
          </op>
  </test>
  <test>
    <op name="convexhull" arg1="A">
      POLYGON ((180 80, 160 500, 780 500, 780 80, 180 80))
          </op>
  </test>
  <test>
    <op name="intersection" arg1="A" arg2="B">
      MULTIPOLYGON (((170.47619047619048 280, 162.85714285714286 440, 320 440, 320 280, 170.47619047619048 280)), 
        ((780 440, 780 280, 620 280, 620 440, 780 440)))
          </op>
  </test>
  <test>
    <op name="union" arg1="A" arg2="B">
      POLYGON ((162.85714285714286 440, 160 500, 320 500, 320 440, 620 440, 620 500, 780 500, 780 440, 840 440, 840 280, 
          780 280, 780 80, 180 80, 170.47619047619048 280, 60 280, 60 440, 162.85714285714286 440), 
        (320 280, 320 200, 620 200, 620 280, 320 280))
          </op>
  </test>
  <test>
    <op name="difference" arg1="A" arg2="B">
      MULTIPOLYGON (((162.85714285714286 440, 160 500, 320 500, 320 440, 162.85714285714286 440)), 
        ((780 280, 780 80, 180 80, 170.47619047619048 280, 320 280, 320 200, 620 200, 620 280, 780 280)), 
        ((620 440, 620 500, 780 500, 780 440, 620 440)))
          </op>
  </test>
  <test>
    <op name="symdifference" arg1="A" arg2="B">
      MULTIPOLYGON (((162.85714285714286 440, 160 500, 320 500, 320 440, 162.85714285714286 440)), 
        ((162.85714285714286 440, 170.47619047619048 280, 60 280, 60 440, 162.85714285714286 440)), 
        ((780 280, 780 80, 180 80, 170.47619047619048 280, 320 280, 320 200, 620 200, 620 280, 780 280)), 
        ((780 280, 780 440, 840 440, 840 280, 780 280)), 
        ((620 440, 620 500, 780 500, 780 440, 620 440)), 
        ((620 440, 620 280, 320 280, 320 440, 620 440)))
          </op>
  </test>
</case>

</run>