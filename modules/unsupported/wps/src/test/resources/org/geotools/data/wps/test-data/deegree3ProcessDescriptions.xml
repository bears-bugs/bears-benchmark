          <ows:DataType ows:reference="http://www.w3.org/TR/xmlschema-2/#integer">integer</ows:DataType>
          <UOMs>
            <Default>
              <ows:UOM>seconds</ows:UOM>
            </Default>
            <Supported>
              <ows:UOM>seconds</ows:UOM>
              <ows:UOM>minutes</ows:UOM>
            </Supported>
          </UOMs>
          <ows:AnyValue/>
        </LiteralData>
      </Input>
